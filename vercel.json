{
    "build": {
      "env": {
        "NPM_CONFIG_FROZEN_LOCKFILE": "false"
      }
    },
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "http://51.20.82.46:5000/:path*"
      }
    ],
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
  
  